	email = steven.merrill@gmail.com
[color]
	ui = true
[core]
	excludesfile = /Users/smerrill/.gitignore_global
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
